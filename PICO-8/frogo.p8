pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
frog_sprite=1
frog_x=-8
frog_y=59
frog_anim_time=0
frog_anim_wait=.05

function _update()
  frog_x+=1
  if frog_x > 127 then
    frog_x=-8
  end

  if time() - frog_anim_time > frog_anim_wait then
    frog_sprite+=1
    frog_anim_time=time()

    if frog_sprite > 9 then
      frog_sprite=1
    end
  end
end

function _draw()
  cls()
  spr(frog_sprite, frog_x, frog_y)
  print( time() )
end

